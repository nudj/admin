{
  "name": "admin",
  "version": "0.0.0",
  "description": "Nudj admin",
  "main": "app/server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nudj/admin.git"
  },
  "author": "Nick Collings",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nudj/admin/issues"
  },
  "homepage": "https://github.com/nudj/admin#readme",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [ "flowtype" ],
    "ignore": [
      "app/server/assets",
      "app/server/build*",
      "app/server/hot*",
      "app/node_modules",
      "flow-typed",
      "webpack*"
    ]
  },
  "scripts": {
    "start": "yarn run build:dlls && yarn run build:client && node .",
    "dev": "yarn run build:dlls && USE_DEV_SERVER=true DEV_SERVER_PATH=https://admin-wds.local.nudj.co yarn run watch:all",
    "build:client": "webpack --config ./webpack.config.client.js --bail --hide-modules",
    "build:dlls": "webpack --config ./webpack.config.dll.js --bail --hide-modules",
    "watch:all": "webpack-dev-server --config ./webpack.config.client.js & webpack --config ./webpack.config.server.js"
  },
  "dependencies": {
    "@nudj/api": "16.0.0",
    "@nudj/components": "2.12.0",
    "@nudj/framework": "17.9.0",
    "@nudj/library": "7.0.0",
    "axios": "^0.16.1",
    "babel-preset-env": "^1.1.10",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.1",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.3",
    "envkey": "^1.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.14.1",
    "flow-bin": "^0.59.0",
    "form-serialize": "^0.7.1",
    "intercom-client": "^2.8.7",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "mailgun-js": "^0.9.1",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "prismic.io": "^3.5.6",
    "prop-types": "^15.5.9",
    "query-string": "5.0.1",
    "react": "^15.4.2",
    "react-autosuggest": "^9.3.1",
    "react-clipboard.js": "^1.1.1",
    "react-content-loader": "^1.3.1",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-textarea-autosize": "5.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "validator": "^7.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "dirty-chai": "^1.2.2",
    "eslint-plugin-flowtype": "2.39.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.14",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "react-hot-loader": "^4.0.0",
    "standard": "^9.0.0",
    "start-server-webpack-plugin": "^2.2.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.6.0"
  }
}
